<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chendy.dao.CitySelectDao">
	<select id="queryAll" resultType="CitySelect">
		SELECT * FROM ytyy_city_select
	</select>
	
	<select id="queryCityById" resultType="CitySelect">
		select *
		from ytyy_city_select
		where id=#{id}
	</select>
</mapper>